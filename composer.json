{
    "name": "muffin/tokenize",
    "description": "Security Tokens for CakePHP",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "muffin",
        "tokenize"
    ],
    "homepage": "https://github.com/usemuffin/tokenize",
    "license": "MIT",
    "authors": [
        {
            "name": "Jad Bitar",
            "homepage": "http://jadb.io",
            "role": "Author"
        },
        {
            "name": "ADmad",
            "homepage": "https://github.com/ADmad",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/usemuffin/tokenize/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/usemuffin/tokenize/issues",
        "source": "https://github.com/usemuffin/tokenize"
    },
    "require": {
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^4.0",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "~8.5.0",
        "psalm/phar": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Muffin\\Tokenize\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muffin\\Tokenize\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
